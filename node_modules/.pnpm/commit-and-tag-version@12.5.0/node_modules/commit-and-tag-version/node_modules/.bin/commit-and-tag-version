#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sansa/projects/precepto/node_modules/.pnpm/commit-and-tag-version@12.5.0/node_modules/commit-and-tag-version/bin/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/commit-and-tag-version@12.5.0/node_modules/commit-and-tag-version/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/commit-and-tag-version@12.5.0/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sansa/projects/precepto/node_modules/.pnpm/commit-and-tag-version@12.5.0/node_modules/commit-and-tag-version/bin/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/commit-and-tag-version@12.5.0/node_modules/commit-and-tag-version/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/commit-and-tag-version@12.5.0/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
