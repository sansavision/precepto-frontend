#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sansa/projects/precepto/node_modules/.pnpm/vitest@2.1.4_@types+node@22.9.1_@vitest+ui@2.1.4_jsdom@25.0.1_terser@5.36.0/node_modules/vitest/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/vitest@2.1.4_@types+node@22.9.1_@vitest+ui@2.1.4_jsdom@25.0.1_terser@5.36.0/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sansa/projects/precepto/node_modules/.pnpm/vitest@2.1.4_@types+node@22.9.1_@vitest+ui@2.1.4_jsdom@25.0.1_terser@5.36.0/node_modules/vitest/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/vitest@2.1.4_@types+node@22.9.1_@vitest+ui@2.1.4_jsdom@25.0.1_terser@5.36.0/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
