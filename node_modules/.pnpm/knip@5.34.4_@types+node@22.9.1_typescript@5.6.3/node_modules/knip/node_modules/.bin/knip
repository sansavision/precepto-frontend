#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sansa/projects/precepto/node_modules/.pnpm/knip@5.34.4_@types+node@22.9.1_typescript@5.6.3/node_modules/knip/bin/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/knip@5.34.4_@types+node@22.9.1_typescript@5.6.3/node_modules/knip/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/knip@5.34.4_@types+node@22.9.1_typescript@5.6.3/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/sansa/projects/precepto/node_modules/.pnpm/knip@5.34.4_@types+node@22.9.1_typescript@5.6.3/node_modules/knip/bin/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/knip@5.34.4_@types+node@22.9.1_typescript@5.6.3/node_modules/knip/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/knip@5.34.4_@types+node@22.9.1_typescript@5.6.3/node_modules:/home/sansa/projects/precepto/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/knip.js" "$@"
else
  exec node  "$basedir/../../bin/knip.js" "$@"
fi
